{
  "name": "cirrax-postfix",
  "version": "1.1.3",
  "author": "Cirrax GmbH",
  "license": "GPL-3.0",
  "summary": "A module to configure postfix mail",
  "source": "https://github.com/cirrax/puppet-postfix",
  "project_page": "https://github.com/cirrax/puppet-postfix",
  "issues_url": "https://github.com/cirrax/puppet-postfix/issues",
  "tags": [
    "postfix",
    "mail",
    "mta"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "Ubuntu"
    },
    {
      "operatingsystem": "OpenBSD",
      "operatingsystemrelease": [
        "6.0",
        "6.1",
        "6.2",
        "6.3"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=4.0.0 < 6.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 3.2.0 < 5.0.0"
    },
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 2.0.0 < 3.0.0"
    }
  ]
}
